{
  "compression": "GZ",
  "directories": [
    "src"
  ],
  "files": [
    "LICENSE"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "tests",
        "Tests",
        "test"
      ],
      "in": "vendor"
    }
  ],
  "git-version": "package_version",
  "intercept": true,
  "output": "phuml.phar"
}
